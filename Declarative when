pipeline {
agent any
environment {
  branch = 'main'
  url = 'https://github.com/Deepika-2998/jenkinsfile.git'
  id = 'github_devpos-oct-toke'
}
stages {
  stage ('build') {
    steps {
      script {
        git url: "${env.url}",
            branch: "${env.branch}",
            credentialsId: "${env.id}"
      }
    }
  }
  stage ('deploy') {
    when {
      branch 'main'
      expression {
        env.GIT_BRANCH='origin/main'
      }
    }
    steps {
        echo "hello"
    }
  }
}
}
